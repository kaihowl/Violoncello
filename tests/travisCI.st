| gitPath |gitPath := (FileDirectory default directoryNamed: 'git_cache') fullName.printDirectories := [:dir |     (dir directories size = 0) ifTrue:[        Transcript show: dir printString.]    ifFalse: [        Transcript show: dir printString.        dir directories do: [:sub | printDirectories value: sub]]].printDirectories value: gitPath.Metacello new  configuration: 'Violoncello';  repository: 'filetree://', gitPath, '/Violoncello/repository';  load: 'Tests'.TravisCIHarness  value: #( 'ConfigurationOfVioloncello' )  value: 'TravisCISuccess.txt'   value: 'TravisCIFailure.txt'.